extends МашинаРеквизитов.КомандыУправления.Интерфейс;

  Import МашинаРеквизитов;
  Import Базовый;

InClass Public


  proc СоздатьКоманды;

    var Иконка: string;
    Иконка = BaseInfo.ProjectByName["КлиентБанкПлюс"].Folder+"Приложения\Иконки\";

    extended СоздатьКоманды;
    with КомандыУправления.ОткрытьКартотеку do

    end;
    with КомандыУправления.ОткрытьОтчет do
    end;

     with КомандыУправления.ОткрытьБланк do
     Добавить("ТФ.Экспортбанковскихдокументов", "Экспорт банковских документов", КлиентБанк.Документы.КлиентБанк.блЭкспорт,, Иконка + "Экспорт банковских документов");
     Добавить("ТФ.Импортбанковскихдокументов",  "Импорт банковских документов",  КлиентБанк.Документы.КлиентБанк.блИмпорт,, Иконка + "Импорт банковских документов");
     Добавить("ТФ.Настройкаэкспорта",           "Настройка экспорта",            КлиентБанк.Документы.КлиентБанк.блЭкспНастр,, Иконка + "Настройка экспорта");
     Добавить("ТФ.Настройкаимпорта",            "Настройка импорта",             КлиентБанк.Документы.КлиентБанк.блИмпНастр,, Иконка + "Настройка импорта");

    end;


   end;

  proc СоздатьПанели;
    var Иконка: string;
    Иконка = BaseInfo.ProjectByName["КлиентБанкПлюс"].Folder+"Приложения\Иконки\";
    extended СоздатьПанели;
     with КомандыУправления.ПанельКоманд.Взять("Деньги",, Иконка + "Банк" , 1) do
           with ВзятьГруппу("Клиент-Банк плюс",,,60) do
           ДобавитьКоманды([
                   "ТФ.Экспортбанковскихдокументов",
                   "ТФ.Импортбанковскихдокументов",
                   "ТФ.Настройкаэкспорта",
                   "ТФ.Настройкаимпорта"
                              ]);
            end;
     end;
  end;
inobject

end